# .pylintrc
[MASTER]
ignore=tests

[MESSAGES CONTROL]
disable=
    C0111,  # Missing docstring
    C0116,  # Missing function or method docstring
    E0401,   # Unable to import
    W0621,   # Refefining names
    R1736,   # Unnecesasary list lookup
    R0902,  # Too many instance attributes
    R0913, # Too many arguments
    R0917  # Too mant positional arguments

[FORMAT]
max-line-length=120
